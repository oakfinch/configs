{
  "name": "@oakfinch/tsconfig",
  "version": "1.0.10",
  "description": "Base tsconfig for oakfinch projects",
  "main": "index.js",
  "sideEffects": false,
  "files": [
    "tsconfig.json",
    "esm",
    "cjs"
  ],
  "scripts": {
    "build:clean": "rm -rf esm && rm -rf cjs && rm -f tsconfig.json",
    "build:cjs": "mkdirp cjs && cp tsconfig.cjs.json cjs/tsconfig.json && cp tsconfig.cjs.json tsconfig.json",
    "build:esm": "mkdirp esm && cp tsconfig.esm.json esm/tsconfig.json",
    "build": "npm run build:clean && npm run build:cjs && npm run build:esm",
    "test": "exit 0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oakfinch/tsconfig.git"
  },
  "keywords": [
    "TypeScript",
    "tsconfig",
    "tsconfig.json"
  ],
  "author": "Nate Volker <natevolker@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/oakfinch/tsconfig/issues"
  },
  "homepage": "https://github.com/oakfinch/tsconfig#readme",
  "peerDependencies": {
    "typescript": ">=4.4.3"
  },
  "devDependencies": {
    "mkdirp": "^1.0.4"
  }
}
