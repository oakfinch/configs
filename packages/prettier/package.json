{
  "name": "@oakfinch/prettier-config",
  "version": "1.0.4",
  "description": "Shared base prettier config for @oakfinch projects",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "build:clean": "rm -rf dist",
    "build:cjs": "echo \"module.exports = require('../src');\" > dist/index.js",
    "build:esm": "echo \"export {default} from './index.js';\" > dist/index.mjs",
    "build:types": "echo \"export {default} from './index.js';\" > dist/index.d.ts",
    "build": "npm run build:clean && mkdirp dist && npm run build:cjs && npm run build:esm && npm run build:types"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oakfinch/prettier-config.git"
  },
  "author": "Nate Volker <natevolker@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "prettier-airbnb-config": "^1.0.0"
  },
  "peerDependencies": {
    "prettier": "^1.18.2"
  },
  "devDependencies": {
    "mkdirp": "^1.0.4",
    "prettier": "^1.18.2"
  }
}
