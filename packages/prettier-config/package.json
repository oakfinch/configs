{
  "name": "@oakfinch/prettier-config",
  "version": "1.0.3",
  "description": "Shared base prettier config for oakfinch projects",
  "main": "./src/exports/config.js",
  "module": "./src/exports/config.mjs",
  "types": "./src/exports/config.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./src/exports/config.mjs",
      "require": "./src/exports/config.cjs",
      "default": "./src/exports/config.js"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "./src/exports/config.d.qts"
      ]
    }
  },
  "files": [
    "src"
  ],
  "scripts": {
    "lint": "eslint --ext js,cjs,mjs,ts . && prettier -c \"**\"",
    "lint:fix": "eslint --ext js,cjs,mjs,ts --fix . && prettier --write \"**\"",
    "typecheck": "exit 0",
    "test": "exit 0",
    "pre-commit": "lint-staged",
    "commit-msg": "commitlint -e",
    "pre-push": "npm run lint && npm run typecheck && npm run test",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oakfinch/prettier-config.git"
  },
  "author": "Nate Volker <natevolker@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "prettier-airbnb-config": "^1.0.0"
  },
  "peerDependencies": {
    "prettier": "^1.18.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.2.1",
    "@commitlint/config-conventional": "^13.2.0",
    "@oakfinch/eslint-config": "^1.1.6",
    "@oakfinch/tsconfig": "^1.0.7",
    "eslint": "^7.2.0",
    "husky": "^7.0.0",
    "lint-staged": "^11.2.4",
    "prettier": "^1.18.2"
  }
}
