{
  "name": "@oakfinch/eslint-config",
  "version": "1.1.0",
  "description": "Base ESLint config for oakfinch projects",
  "main": ".eslintrc.js",
  "module": ".eslintrc.mjs",
  "types": ".eslintrc.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./.eslintrc.mjs",
      "require": "./.eslintrc.js"
    },
    "./typescript": {
      "import": "./.eslintrc.typescript.mjs",
      "require": "./.eslintrc.typescript.js"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "./.eslintrc.d.ts"
      ],
      "./typescript": [
        "./.eslintrc.typescript.d.ts"
      ]
    }
  },
  "files": [
    "./.eslintrc.js",
    "./.eslintrc.mjs",
    "./.eslintrc.d.ts",
    "./.eslintrc.typescript.js",
    "./.eslintrc.typescript.mjs",
    "./.eslintrc.typescript.d.ts"
  ],
  "scripts": {
    "build": "exit 0",
    "test": "exit 0",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oakfinch/eslint-config.git"
  },
  "keywords": [
    "ESLint"
  ],
  "author": "Nate Volker <natevolker@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/oakfinch/eslint-config/issues"
  },
  "homepage": "https://github.com/oakfinch/eslint-config#readme",
  "peerDependencies": {
    "eslint": ">=7.32.0",
    "typescript": ">=4.4.3"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^14.0.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-json": "^3.1.0"
  },
  "devDependencies": {
    "eslint": "^8.0.1",
    "typescript": "^4.4.4"
  }
}
