{
  "name": "@oakfinch/eslint-config",
  "version": "1.1.4",
  "description": "Base ESLint config for oakfinch projects",
  "main": "./src/exports/javascript.js",
  "module": "./src/exports/javascript.mjs",
  "types": "./src/exports/javascript.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./src/exports/javascript.mjs",
      "require": "./src/exports/javascript.cjs",
      "default": "./src/exports/javascript.js"
    },
    "./typescript": {
      "import": "./src/exports/typescript.mjs",
      "require": "./src/exports/typescript.cjs",
      "default": "./src/exports/typescript.js"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "./src/exports/javascript.d.ts"
      ],
      "./typescript": [
        "./src/exports/typescript.d.ts"
      ]
    }
  },
  "files": [
    "src"
  ],
  "scripts": {
    "build": "exit 0",
    "test": "exit 0",
    "lint": "eslint . --ext js,mjs,cjs,ts,json",
    "lint:fix": "eslint . --ext js,mjs,cjs,ts,json --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oakfinch/eslint-config.git"
  },
  "keywords": [
    "ESLint"
  ],
  "author": "Nate Volker <natevolker@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/oakfinch/eslint-config/issues"
  },
  "homepage": "https://github.com/oakfinch/eslint-config#readme",
  "peerDependencies": {
    "eslint": ">=8.0.0",
    "prettier": ">=2.0.0",
    "typescript": ">=4.4.0"
  },
  "dependencies": {
    "@oakfinch/prettier-config": "^1.0.1",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^14.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-prettier": "^4.0.0"
  },
  "devDependencies": {
    "@oakfinch/tsconfig": "^1.0.7",
    "eslint": "^8.1.0",
    "prettier": "^2.4.1",
    "typescript": "^4.4.4"
  }
}
